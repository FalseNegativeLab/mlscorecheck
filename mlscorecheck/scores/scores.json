{
    "scores": {
        "acc": {
            "name": "accuracy",
            "abbreviation": "acc",
            "lower_bound": 0,
            "upper_bound": 1,
            "complement": "err",
            "args": ["tp", "tn", "p", "n"],
            "formula": "(tp + tn)/(p + n)",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "(tp + tn)/(p + n)",
            "polynomial_equation": "acc*(p + n) - (tp + tn)"
        },
        "err": {
            "name": "error_rate",
            "abbreviation": "err",
            "lower_bound": 0,
            "upper_bound": 1,
            "complement": "acc",
            "args": ["fp", "fn", "p", "n"],
            "formula": "(fp + fn)/(p + n)",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "1 - (tp + tn)/(p + n)"
        },
        "sens": {
            "name": "sensitivity",
            "abbreviation": "sens",
            "lower_bound": 0,
            "upper_bound": 1,
            "complement": "fnr",
            "args": ["tp", "p"],
            "formula": "tp/p",
            "args_standardized": ["tp", "p"],
            "formula_standardized": "tp/p",
            "synonyms": ["recall", "true_positive_rate"],
            "polynomial_equation": "sens*p - tp"
        },
        "fnr": {
            "name": "false_negative_rate",
            "abbreviation": "fnr",
            "lower_bound": 0,
            "upper_bound": 1,
            "complement": "sens",
            "args": ["fn", "p"],
            "formula": "fn/p",
            "args_standardized": ["tp", "p"],
            "formula_standardized": "1 - tp/p",
            "synonyms": ["miss_rate"]
        },
        "fpr": {
            "name": "false_positive_rate",
            "abbreviation": "fpr",
            "lower_bound": 0,
            "upper_bound": 1,
            "complement": "spec",
            "args": ["fp", "n"],
            "formula": "fp/n",
            "args_standardized": ["tn", "n"],
            "formula_standardized": "1 - tn/n",
            "synonyms": ["false_alarm", "fall_out"]
        },
        "spec": {
            "name": "specificity",
            "abbreviation": "spec",
            "lower_bound": 0,
            "upper_bound": 1,
            "complement": "fpr",
            "args": ["tn", "n"],
            "formula": "tn/n",
            "args_standardized": ["tn", "n"],
            "formula_standardized": "tn/n",
            "synonyms": ["selectivity", "true_negative_rate"],
            "polynomial_equation": "spec*n - tn"
        },
        "ppv": {
            "name": "positive_predictive_value",
            "abbreviation": "ppv",
            "lower_bound": 0,
            "upper_bound": 1,
            "nans": [{"tp": 0, "fp": 0}],
            "complement": "fdr",
            "args": ["tp", "fp"],
            "formula": "tp/(tp + fp)",
            "args_standardized": ["tp", "tn", "n"],
            "formula_standardized": "tp/(tp + n - tn)",
            "nans_standardized": [{"tp": 0, "tn": "n"}],
            "synonyms": ["precision"],
            "polynomial_equation": "ppv * (tp + n - tn) - tp"
        },
        "fdr": {
            "name": "false_discovery_rate",
            "abbreviation": "fdr",
            "lower_bound": 0,
            "upper_bound": 1,
            "nans": [{"tp": 0, "fp": 0}],
            "complement": "ppv",
            "args": ["tp", "fp"],
            "formula": "fp/(tp + fp)",
            "args_standardized": ["tp", "tn", "n"],
            "formula_standardized": "1 - tp/(tp + n - tn)",
            "nans_standardized": [{"tp": 0, "tn": "n"}]
        },
        "for_": {
            "name": "false_omission_rate",
            "abbreviation": "for_",
            "lower_bound": 0,
            "upper_bound": 1,
            "nans": [{"tn": 0, "fn": 0}],
            "complement": "npv",
            "args": ["tn", "fn"],
            "formula": "fn/(tn + fn)",
            "args_standardized": ["tp", "tn", "p"],
            "formula_standardized": "1 - tn/(tn + p - tp)",
            "nans_standardized": [{"tp": "p", "tn": 0}]
        },
        "npv": {
            "name": "negative_predictive_value",
            "abbreviation": "npv",
            "lower_bound": 0,
            "upper_bound": 1,
            "nans": [{"tn": 0, "fn": 0}],
            "complement": "for_",
            "args": ["tn", "fn"],
            "formula": "tn/(tn + fn)",
            "args_standardized": ["tp", "tn", "p"],
            "formula_standardized": "tn/(tn + p - tp)",
            "nans_standardized": [{"tp": "p", "tn": 0}],
            "polynomial_equation": "npv * (tn + p - tp) - tn"
        },
        "fbp": {
            "name": "f_beta_plus",
            "abbreviation": "fbp",
            "lower_bound": 0,
            "args": ["tp", "fp", "p", "beta_plus"],
            "formula": "((1 + beta_plus**2)*tp) / (tp + beta_plus**2*p + fp)",
            "args_standardized": ["tp", "tn", "p", "n", "beta_plus"],
            "formula_standardized": "((1 + beta_plus**2)*tp) / (tp + beta_plus**2*p + n - tn)",
            "polynomial_equation": "fbp * (tp + beta_plus**2*p + n - tn) - ((1 + beta_plus**2)*tp)"
        },
        "fbm": {
            "name": "f_beta_minus",
            "abbreviation": "fbm",
            "lower_bound": 0,
            "args": ["tn", "fn", "n", "beta_minus"],
            "formula": "((1 + beta_minus**2)*tn) / (tn + beta_minus**2*n + fn)",
            "args_standardized": ["tp", "tn", "p", "n", "beta_minus"],
            "formula_standardized": "((1 + beta_minus**2)*tn) / (tn + beta_minus**2*n + p - tp)",
            "polynomial_equation": "fbm * (tn + beta_minus**2*n + p - tp) - ((1 + beta_minus**2)*tn)"
        },
        "f1p": {
            "name": "f1_plus",
            "abbreviation": "f1p",
            "lower_bound": 0,
            "args": ["tp", "fp", "p"],
            "formula": "(2*tp) / (tp + p + fp)",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "(2*tp) / (tp + p + n - tn)",
            "synonyms": ["f1"],
            "polynomial_equation": "f1p * (tp + p + n - tn) - 2*tp"
        },
        "f1m": {
            "name": "f1_minus",
            "abbreviation": "f1m",
            "lower_bound": 0,
            "args": ["tn", "fn", "n"],
            "formula": "(2*tn) / (tn + n + fn)",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "(2*tn) / (tn + n + p - tp)",
            "polynomial_equation": "f1m * (tn + n + p - tp) - 2*tn"
        },
        "upm": {
            "name": "unified_performance_measure",
            "abbreviation": "upm",
            "lower_bound": 0,
            "nans": [{"tp": 0, "tn": 0}],
            "short_args": ["f1p", "f1m"],
            "short_formula": "2*f1p*f1m/(f1p + f1m)",
            "args": ["tp", "tn", "p", "n"],
            "formula": "4*tn*tp/(tn*(n + p - tn + tp) + tp*(n + p + tn - tp))",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "4*tn*tp/(tn*(n + p - tn + tp) + tp*(n + p + tn - tp))",
            "nans_standardized": [{"tp": 0, "tn": 0}],
            "polynomial_equation": "upm * (tn*(n + p - tn + tp) + tp*(n + p + tn - tp)) - 4*tp*tn"
        },
        "gm": {
            "name": "geometric_mean",
            "abbreviation": "gm",
            "lower_bound": 0,
            "upper_bound": 1,
            "sqrt": true,
            "short_args": ["sens", "spec"],
            "short_formula": "sqrt(sens*spec)",
            "args": ["tp", "tn", "p", "n"],
            "formula": "sqrt(tp)*sqrt(tn)/(sqrt(p)*sqrt(n))",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "sqrt(tp)*sqrt(tn)/(sqrt(p)*sqrt(n))",
            "polynomial_equation": "gm**2 * p * n - tp * tn"
        },
        "fm": {
            "name": "fowlkes_mallows_index",
            "abbreviation": "fm",
            "lower_bound": 0,
            "upper_bound": 1,
            "nans": [{"tp": 0, "fp": 0}],
            "sqrt": true,
            "short_args": ["ppv", "tpr"],
            "short_formula": "sqrt(ppv*tpr)",
            "args": ["tp", "fp", "p"],
            "formula": "tp/sqrt(p*(fp + tp))",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "tp/sqrt(p*(n - tn + tp))",
            "nans_standardized": [{"tp": 0, "tn": "n"}],
            "polynomial_equation": "fm**2 * (p*(n - tn + tp)) - tp**2"
        },
        "mk": {
            "name": "markedness",
            "abbreviation": "mk",
            "lower_bound": 0,
            "upper_bound": 1,
            "nans": [{"tp": 0, "fp": 0}],
            "short_args": ["ppv", "npv"],
            "short_formula": "ppv + npv - 1",
            "args": ["tp", "tn", "p", "n"],
            "formula": "tp/(tp + n - tn) + tn/(tn + p - tp) - 1",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "tp/(tp + n - tn) + tn/(tn + p - tp) - 1",
            "nans_standardized": [{"tp": 0, "tn": "n"}, {"tp": "p", "tn": 0}],
            "synonyms": ["delta_p"],
            "polynomial_equation": "mk*(tp + n - tn)*(tn + p - tp) - tp*(tn + p - tp) - tn*(tp + n - tn) + (tp + n - tn)*(tn + p - tp)"
        },
        "lrp": {
            "name": "positive_likelihood_ratio",
            "abbreviation": "lrp",
            "lower_bound": 0,
            "nans": [{"fp": 0}],
            "short_args": ["tpr", "fpr"],
            "short_formula": "tpr/fpr",
            "args": ["tp", "fp", "p", "n"],
            "formula": "n*tp/(fp*p)",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "n*tp/((n-tn)*p)",
            "nans_standardized": [{"tn": "n"}],
            "polynomial_equation": "lrp * ((n-tn)*p) - n*tp"
        },
        "lrn": {
            "name": "negative_likelihood_ratio",
            "abbreviation": "lrn",
            "lower_bound": 0,
            "nans": [{"tn": 0}],
            "short_args": ["tnr", "fnr"],
            "short_formula": "fnr/tnr",
            "args": ["tn", "fn", "p", "n"],
            "formula": "n*fn/(tn*p)",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "n*(p - tp)/(tn*p)",
            "nans_standardized": [{"tn": 0}],
            "polynomial_equation": "lrn*(tn*p) - n*(p - tp)"
        },
        "mcc": {
            "name": "matthews_correlation_coefficient",
            "abbreviation": "mcc",
            "lower_bound": -1,
            "upper_bound": 1,
            "nans": [{"tp": 0, "fp": 0}, {"tn": 0, "fn": 0}],
            "sqrt": true,
            "short_args": ["tpr", "tnr", "ppv", "npv", "fnr", "fpr", "for_", "fdr"],
            "short_formula": "sqrt(tpr*tnr*ppv*npv) - sqrt(fnr*fpr*for_*fdr)",
            "args": ["tp", "tn", "p", "n"],
            "formula": "(tn*tp - (n - tn)*(p - tp))/sqrt(n*p*(n - tn + tp)*(p + tn - tp))",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "(tn*tp - (n - tn)*(p - tp))/sqrt(n*p*(n - tn + tp)*(p + tn - tp))",
            "nans_standardized": [{"tp": 0, "tn": "n"}, {"tp": "p", "tn": 0}],
            "polynomial_equation": "mcc**2 * (n*p*(n - tn + tp)*(p + tn - tp)) - (tn*tp - (n-tn)*(p-tp))**2"
        },
        "bm": {
            "name": "bookmaker_informedness",
            "abbreviation": "bm",
            "lower_bound": 0,
            "upper_bound": 1,
            "short_args": ["tpr", "tnr"],
            "short_formula": "tpr + tnr - 1",
            "args": ["tp", "tn", "p", "n"],
            "formula": "tp/p + tn/n - 1",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "tp/p + tn/n - 1",
            "synonyms": ["informedness"],
            "polynomial_equation": "bm*p*n - tp*n - tn*p + p*n"
        },
        "pt": {
            "name": "prevalence_threshold",
            "abbreviation": "pt",
            "sqrt": true,
            "nans": [{"fp": 0, "tp": 0}, {"tp": "p", "fp": "n"}],
            "short_args": ["tpr", "fpr"],
            "short_formula": "(sqrt(tpr*fpr) - fpr)/(tpr - fpr)",
            "args": ["tp", "fp", "p", "n"],
            "formula": "-p*(-fp + n*sqrt(fp*tp/(n*p)))/(fp*p - n*tp)",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "-p*(n*sqrt(tp*(n - tn)/(n*p)) - n + tn)/(-n*tp + p*(n - tn))",
            "nans_standardized": [{"tp": 0, "tn": "n"}, {"tp": "p", "tn": 0}],
            "polynomial_equation": "(pt*(-n*tp + p*(n-tn)) - p*n + p*tn)**2 - p*n*(tp*(n-tn))"
        },
        "dor": {
            "name": "diagnostic_odds_ratio",
            "abbreviation": "dor",
            "lower_bound": 0,
            "nans": [{"tn": "n"}, {"tp": "p"}],
            "short_args": ["lrp", "lrn"],
            "short_formula": "lrp/lrn",
            "args": ["tp", "tn", "p", "n"],
            "formula": "tn*tp/((n-tn)*(p-tp))",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "tn*tp/((n-tn)*(p-tp))",
            "nans_standardized": [{"tn": "n"}, {"tp": "p"}],
            "polynomial_equation": "dor*(n-tn)*(p-tp) - tn*tp"
        },
        "ji": {
            "name": "jaccard_index",
            "abbreviation": "ji",
            "lower_bound": 0,
            "args": ["tp", "fp", "p"],
            "formula": "tp/(fp + p)",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "tp / (n - tn + p)",
            "synonyms": ["threat_score"],
            "polynomial_equation": "ji*(n-tn+p) - tp"
        },
        "bacc": {
            "name": "balanced_accuracy",
            "abbreviation": "bacc",
            "lower_bound": 0,
            "upper_bound": 1,
            "short_args": ["sens", "spec"],
            "short_formula": "(sens + spec)/2",
            "args": ["tp", "tn", "p", "n"],
            "formula": "tp/(2*p) + tn/(2*n)",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "tp/(2*p) + tn/(2*n)",
            "polynomial_equation": "bacc*2*p*n - tp*n - tn*p"
        },
        "kappa": {
            "name": "cohens_kappa",
            "abbreviation": "kappa",
            "nans": [{"tn": "n", "tp": 0}, {"tp": "p", "tn": 0}],
            "args": ["tp", "tn", "p", "n"],
            "formula": "(2*tn*tp - 2*(n - tn)*(p - tp))/(n*(n - tn + tp) + p*(p + tn - tp))",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "-2*(n*p - n*tp - p*tn)/(n**2 - n*tn + n*tp + p**2 + p*tn - p*tp)",
            "nans_standardized": [{"tn": "n", "tp": 0}, {"tp": "p", "tn": 0}],
            "polynomial_equation": "kappa * (n**2 - n*tn + n*tp + p**2 + p*tn - p*tp) + 2*(n*p - n*tp - p*tn)"
        },
        "p4": {
            "name": "p4",
            "abbreviation": "p4",
            "nans": [{"tp": 0, "tn": 0}, {"tp": "p", "tn": "n"}],
            "lower_bound": 0,
            "upper_bound": 1,
            "args": ["tp", "tn", "p", "n"],
            "formula": "4*tp*tn / (4*tp*tn + (tp + tn)*(p - tp + n - tn))",
            "args_standardized": ["tp", "tn", "p", "n"],
            "formula_standardized": "4*tp*tn / (4*tp*tn + (tp + tn)*(p - tp + n - tn))",
            "nans_standardized": [{"tp": 0, "tn": 0}, {"tp": "p", "tn": "n"}],
            "polynomial_equation": "p4 * (4*tp*tn + (tp + tn)*(p - tp + n - tn)) - 4*tp*tn"
        }
    }
}
